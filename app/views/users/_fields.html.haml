= render 'layouts/error_messages', object: f.object

.form-group
  = f.label :name, class: 'col-lg-2 control-label'
  .col-lg-10
    = f.text_field :name
.form-group
  = f.label :email, class: 'col-lg-2 control-label'
  .col-lg-10
    = f.text_field :email
.form-group
  = f.label :password, class: 'col-lg-2 control-label'
  .col-lg-10
    = f.password_field :password